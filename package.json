{
  "name": "express-authorshaven",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node ./build/server.js",
    "dev": "babel-watch server.js",
    "build": "npm run clean && mkdir build && babel ./ --ignore 'node_modules,build,.github,.nyc_output' -s -d build",
    "clean": "rm -rf build",
    "test": "nyc mocha --require babel-register --exit",
    "test:dev": "NODE_ENV=test npm run test",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:testdb": "NODE_ENV=test npm run db:migrate",
    "undo:migrations": "sequelize db:migrate:undo:all",
    "undo:migrations:testdb": "NODE_ENV=test npm run undo:migrations"
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^6.3.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-watch": "^2.0.8",
    "bcrypt": "^3.0.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "ejs": "^2.6.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^8.4.0",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^7.7.1",
    "pg-hstore": "^2.3.2",
    "request": "^2.87.0",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "slug": "^0.9.1",
    "supertest": "^3.3.0",
    "underscore": "^1.9.1",
    "validator": "^10.10.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^5.12.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^13.1.0"
  }
}
